{
    "compilerOptions": {
        "target": "ES2016",
        "module": "CommonJS",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": false,
        "noImplicitAny": false,
        "skipLibCheck": true,
        "rootDir": "src",
        "outDir": "out",
        "removeComments": true
    },
}